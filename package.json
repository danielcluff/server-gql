{
  "name": "graphql-server",
  "version": "1.0.0",
  "description": "GraphQL server built on Node, Fastify, Helix, TypeGraphQL, TypeOrm",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "start": "node dist/index.js",
    "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "argon2": "^0.30.3",
    "class-validator": "^0.14.0",
    "connect-redis": "^7.1.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.20.0",
    "graphql": "^15.8.0",
    "graphql-helix": "^1.13.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "@types/uuid": "^9.0.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
